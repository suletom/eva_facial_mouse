    <string name="wizard_set_keyboard">
        <![CDATA[1. Nyisd meg: \'Beállítások > Nyelv és bevitel\']]>
        \n2. Kapcsold be \'EVA Billentyűzet\'
        \n3. Kattints rá \'Alapértelmezett\'
        \n4. Válaszd ki \'EVA Billentyűzet\'
    </string>
    <string name="wizard_set_keyboard_lollipop">
        <![CDATA[1. Open: \'Settings > Language & input > Current Keyboard > CHOOSE KEYBOARDS\']]>
        \n2. Kapcsold be \'EVA Billentyűzet\'
        \n3. Menj vissza
        \n4. Kattints rá \'Aktuális Billentyűzet\'
        \n5. Válaszd ki \'EVA Billentyűzet\'
    </string>
